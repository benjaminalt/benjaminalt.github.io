  booktitle = {Proceedings of {{The}} 6th {{Conference}} on {{Robot Learning}}},
  author = {Jatavallabhula, Krishna Murthy and Macklin, Miles and Fox, Dieter and Garg, Animesh and Ramos, Fabio},
