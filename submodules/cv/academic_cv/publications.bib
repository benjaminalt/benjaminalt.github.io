  shorttitle = {Panoptic-{{PartFormer}}},
  booktitle = {Computer {{Vision}} -- {{ECCV}} 2022},
  author = {Li, Xiangtai and Xu, Shilin and Yang, Yibo and Cheng, Guangliang and Tong, Yunhai and Tao, Dacheng},
