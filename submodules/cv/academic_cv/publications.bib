}

@article{frankoDesignMultiRobotSystem2020,
  title = {Design of a {{Multi-Robot System}} for {{Wind Turbine Maintenance}}},
  author = {Franko, Josef and Du, Shengzhi and Kallweit, Stephan and Duelberg, Enno and Engemann, Heiko},
