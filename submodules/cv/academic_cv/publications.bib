  shorttitle = {Model {{Predictive Actor-Critic}}},
  author = {Morgan, Andrew S. and Nandha, Daljeet and Chalvatzaki, Georgia and D'Eramo, Carlo and Dollar, Aaron M. and Peters, Jan},
  year = {2021},
