  title = {{{iADA}}*: {{Improved Anytime Path Planning}} and {{Replanning Algorithm}} for {{Autonomous Vehicle}}},
  shorttitle = {{{iADA}}*},
  author = {Maw, Aye Aye and Tyan, Maxim and Lee, Jae-Woo},
