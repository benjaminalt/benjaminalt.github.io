}

@inproceedings{irieLSTMGRUHighway2016,
  title = {{{LSTM}}, {{GRU}}, {{Highway}} and a {{Bit}} of {{Attention}}: {{An Empirical Overview}} for {{Language Modeling}} in {{Speech Recognition}}},
