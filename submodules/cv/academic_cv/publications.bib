@article{pairetPathPlanningManipulation2021,
  title = {Path {{Planning}} for {{Manipulation}} Using {{Experience-driven Random Trees}}},
  author = {Pairet, {\`E}ric and Chamzas, Constantinos and Petillot, Yvan and Kavraki, Lydia E.},
  year = {2021},
