  shorttitle = {{{LASER}}},
  author = {Allshire, Arthur and {Mart{\'i}n-Mart{\'i}n}, Roberto and Lin, Charles and Manuel, Shawn and Savarese, Silvio and Garg, Animesh},
  year = {2021},
