  title = {{{DOVE}}: {{Learning Deformable 3D Objects}} by {{Watching Videos}}},
  shorttitle = {{{DOVE}}},
  author = {Wu, Shangzhe and Jakab, Tomas and Rupprecht, Christian and Vedaldi, Andrea},
