@article{jasimPositionIdentificationForceGuided2014,
  title = {Position {{Identification}} in {{Force-Guided Robotic Peg-in-Hole Assembly Tasks}}},
  author = {Jasim, Ibrahim F. and Plapper, Peter W. and Voos, Holger},
  year = {2014},
